<div style="width: 100%; margin: 0 auto; ">
    <div class="col-md-4" align="center">
        <legend>
            <h4 align="center">Schedules for this Driver</h4>
        </legend>
        <table>
            <div id="mainbox">
                <div class="border">
                    {{#each model as |schedule|}}
                        <br/>
                        Vehicle Id: {{schedule.vehicle_id}}
                        <br/>
                        <br/>
                        Start Day: {{schedule.str_starting_at_date}}
                        <br/>
                        <br/>
                        End Day:: {{schedule.str_ending_at_date}}
                        <br/>
                        <br/>
                        <div align="right">
                            <button {{action 'goBack'}} class="btn btn-warning btn-sm navbar-secondary-btn"
                                                        type="submit">
                                Back
                            </button>
                        </div>
                        <p class="bottomBorder"></p>
                    {{/each}}
                </div>
            </div>
        </table>
    </div>
</div>
{{outlet}}